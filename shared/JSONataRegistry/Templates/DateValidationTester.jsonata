(
$DateRegex := /^(\d{4})-(0[1-9]|1[0-2])-(0[1-9]|[12]\d|3[01])T([01]\d|2[0-3]):([0-5]\d):([0-5]\d)(\.\d+)?([+-]([01]\d|2[0-3]):([0-5]\d)|Z)$/;
$DatePicture := "[Y0001]-[M01]-[D01]T[H01]:[m01]:[s01].[f001]Z";
    {
     "RegexValid":$contains(createdAt,$DateRegex),
     "DateConversion":$fromMillis($toMillis(createdAt,$DatePicture))
    }
)